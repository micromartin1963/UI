FrameWidgetClass testuiandinput
{
	visible 0
	color 0 0 0 0
	position 0 0
	size 650 450
	halign center_ref
	valign center_ref
	hexactpos 0
	vexactpos 0
	hexactsize 1
	vexactsize 1
	style EditorPanel

	{
        PanelWidgetClass Background
        {
			visible 1
			color 0 0 0 0.65
			position 0 0
			size 650 450
			hexactpos 1
			vexactpos 1
			hexactsize 0
			vexactsize 1
			style rover_sim_colorable
        }

		PanelWidgetClass title_wrapper
		{
			visible 1
			color 0 0 0 0.7843
			position 0 0
			size 1 50
			hexactpos 1
			vexactpos 1
			hexactsize 0
			vexactsize 1
			style rover_sim_colorable

			{
				TextWidgetClass title_text
				{
					position 0 0
					size 1 1
					halign center_ref
					valign center_ref
					hexactpos 0
					vexactpos 0
					hexactsize 0
					vexactsize 0
					style Normal
					text "Anti Camping Mode"
					font "gui/fonts/Metron48"
					"text offset" 30 0
				}
			}
		}
		

	
		
		ButtonWidgetClass btn_cancel
        {
            position 5 5
            size 160 40
            halign left_ref
            valign bottom_ref
            hexactpos 1
            vexactpos 1
            hexactsize 1
            vexactsize 1
            text "#cancel"
        }
		
		ButtonWidgetClass btn_enable
        {
            position 170 5
            size 160 40
            halign right_ref
            valign bottom_ref
            hexactpos 1
            vexactpos 1
            hexactsize 1
            vexactsize 1
            text "#enable"
        }
		
		ButtonWidgetClass btn_disable
        {
            position 5 5
            size 160 40
            halign right_ref
            valign bottom_ref
            hexactpos 1
            vexactpos 1
            hexactsize 1
            vexactsize 1
            text "#disable"
        }
	}
}